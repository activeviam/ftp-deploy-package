{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:all",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "linebreak-style": "off",
    "no-empty-function": "off",
    "no-ternary": "off",
    "no-prototype-builtins": "off",
    "one-var": "off",
    "prettier/prettier": ["error", {"bracketSpacing": false, "singleQuote": true, "trailingComma": "es5"}]
  }
}
